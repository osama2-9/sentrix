{
  "name": "sentrix",
  "version": "1.0.3",
  "description": "Comprehensive security middleware library for Express.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon src/app.js",
    "start": "node dist/example.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm test",
    "prepare": "npm run build"
  },
  "keywords": [
    "express",
    "security",
    "middleware",
    "csrf",
    "jwt",
    "rate-limiting",
    "xss",
    "validation",
    "authentication",
    "dos-protection",
    "content-security-policy",
    "helmet",
    "security-headers"
  ],
  "author": "Osama Alsrraj <osamaalsrraj3@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/osama2-9/sentrix.git"
  },
  "bugs": {
    "url": "https://github.com/osama2-9/sentrix/issues"
  },
  "homepage": "https://github.com/osama2-9/sentrix#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    ".env.example"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./middleware/*": {
      "import": "./dist/middleware/*.js",
      "types": "./dist/middleware/*.d.ts"
    },
    "./http/*": {
      "import": "./dist/http/*.js",
      "types": "./dist/http/*.d.ts"
    },
    "./security/*": {
      "import": "./dist/security/*.js",
      "types": "./dist/security/*.d.ts"
    },
    "./utils/*": {
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts"
    },
    "./config/*": {
      "import": "./dist/config/*.js",
      "types": "./dist/config/*.d.ts"
    }
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.0",
    "express-rate-limit": "^8.2.1",
    "ioredis": "^5.3.1",
    "jsonwebtoken": "^9.0.0",
    "node-fetch": "^3.3.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "xss": "^1.0.14",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.15.11",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  }
}
